{
  "name": "megabrain-protocol-backend",
  "version": "1.0.0",
  "description": "Off-chain coordination layer for MegaBrain Protocol",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "migrate": "knex migrate:latest",
    "migrate:make": "knex migrate:make",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "cluster:worker": "ts-node src/workers/clusteringWorker.ts",
    "settlement:worker": "ts-node src/workers/settlementWorker.ts"
  },
  "keywords": ["megabrain", "protocol", "ai", "coordination", "blockchain"],
  "author": "MegaBrain Protocol",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^7.1.5",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "ethers": "^6.9.0",
    "knex": "^3.1.0",
    "pg": "^8.11.3",
    "sqlite3": "^5.1.6",
    "ioredis": "^5.3.2",
    "bull": "^4.12.0",
    "axios": "^1.6.2",
    "form-data": "^4.0.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.24.1",
    "sklearn": "^0.2.0",
    "ml-hdbscan": "^1.0.0",
    "ml-distance": "^4.0.0",
    "ml-matrix": "^6.10.6",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "compression": "^1.7.4",
    "node-cron": "^3.0.3",
    "axios-retry": "^4.0.0",
    "eth-sig-util": "^3.0.1",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/uuid": "^9.0.7",
    "@types/node-cron": "^3.0.11",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
